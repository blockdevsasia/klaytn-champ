.content-container {
    padding-left: 40px;
    padding-right: 40px;
}

.purp-button {
	color: white;
	background-color: #2C0099;
	padding: 14px 32px 14px 32px;
}

.content-container {
	max-width: 1000px;
}

.button-dash {
  position: relative;
  padding: 15px 40px;
  text-align: center;
}

.home-button {
	border-radius: 10px;
	padding: 10px;
	filter: drop-shadow(0 0 0.25rem gray);
	background-color: #FFFFFF;
	color: #06042A;
	text-decoration: none!important;
	margin-bottom: 50px;
}

.home-button:hover {
	background-color: #5800FF;
	color: white;
	filter: none;
}

.left-hb {
	margin-right: 40px;
}

.btnimg-dash {
    max-width: 65px;
    display: block;
    margin-left: 24%;
    margin-right: 0;
    padding-top: 10px;
    max-height: 90px;
}

.q-layout__section--marginal {
    background-color: #5800FF !important;
    color: #fff;
}

.singup-txt {
	color: white;
	padding: 36% 0% 0% 27%;
	position: absolute;
}

.footer-singup {
	padding: 20% 27% 20% 6%
	width: 300px;
}

.padding-top {
	padding-top: 80px;
}

.footer-logo {
	padding: 80% 25% 0% 25%;
}

.footer-inner-logo {
    padding: 20px;
    max-width: 170px;
    margin: 20px 0px 0px 55px;
}

.q-drawer--left.q-drawer--bordered {
    border-right: 1px solid #EDEDED;
    background: white;
}

.profile-header {
	border-bottom: 1px solid #EDEDED;
}

.q-item {
    min-height: 70px;
    padding: 8px 16px;
    color: inherit;
    transition: color 0.3s, background-color 0.3s;
}

.q-item:hover {
	background: #5800ff;
	color: white;
}

.profile-pic {
	border: 3px solid #33DDA8;
}

.no-style {
	color: black;
	text-decoration: none;
}

.user-name {
	text-decoration: none;
	color: #33DDA8;
}

.level-indu {
	color: red;
}


@media only screen and (min-width: 1023px) {
	.menu-bar {
		display: none;
	}
}
